{
  "name": "@dev-dep/ui-quarks",
  "version": "0.0.0",
  "description": "Sima-land design tokens as a React components/helpers",
  "publishConfig": {
    "@dev-dep:registry": "https://gitlab.sima-land.ru/api/v4/projects/1151/packages/npm/"
  },
  "repository": "ssh://git@gitlab.sima-land.ru:7888/dev-dep/dev/packages/ui-quarks.git",
  "scripts": {
    "build:before": "rm -rf build && node prebuild.js --tempDir=build-temp",
    "build:main": "tsc --rootDir build-temp --outDir build",
    "build:after": "rm -rf build-temp",
    "build": "npm run build:before && npm run build:main && npm run build:after",
    "dist": "npm run build && cp ./package.json build && cp ./readme.md build",
    "build:docs": "node ./docs/prebuild.js && parcel build ./docs/src/index.html --out-dir ./public --public-url ./ && rm -rf ./docs-temp",
    "watch:docs": "npm run build && node ./docs/prebuild.js && parcel ./docs/src/index.html --out-dir ./public"
  },
  "author": "www.sima-land.ru team",
  "license": "ISC",
  "devDependencies": {
    "@babel/parser": "^7.12.3",
    "@dev-dep/linters": "^1.0.7",
    "@svgr/core": "^5.4.0",
    "@svgr/plugin-svgo": "^5.4.0",
    "@types/fs-extra": "^9.0.1",
    "@types/react": "^16.9.49",
    "clipboard": "^2.0.6",
    "eslint": "^7.9.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "nanoid": "^3.1.16",
    "nanoid-dictionary": "^4.1.0",
    "parcel-bundler": "^1.12.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {},
  "alias": {
    "ui-quarks": "./docs-temp/icons.js"
  }
}
